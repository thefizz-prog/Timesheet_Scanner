            ElseIf (Not IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 1).Value) And Not IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 2).Value) _
                And IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 3).Value) And IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 4).Value) _
                And Not IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 5).Value) And Not IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 6).Value)) Then
                    ' MsgBox "Missing P & L " & vbNewLine & "Row:" & p

                    count = count + 1
                    missingPLAct(counter) = p       ' add row # to array
                    missingPLAct(counter + 1) = "Missing P & L "     ' add description of timesheet error to array
                    counter = counter + 2
                    ' MsgBox "Missing P & L " & vbNewLine & "Row:" & p & vbNewLine & "Count:" & count

