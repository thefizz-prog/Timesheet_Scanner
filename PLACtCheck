Sub PLAct(TheSourceFile, staffCode, currentWorkWeek)


'''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''
'''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''
' This macro checks that the information entered on the staffs timesheet is as expected
' I.e there are not numbers where there should be letters and visa versa.
' Also checks that there is no missing row or column data.
' If there is then fail this test and record the failure in the stafflist workbook
''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''
'''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''



'''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''
' ARGUMENTS
' TheSourceFile = The staff timesheet file path
' staffCode = unique staff identifier
' currentWorkWeek = the current workweek
''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''

    Dim projects As Workbook
    Set projects = Workbooks.Open("C:\Users\tfinucane\Documents\Timesheet Harvesting\Project Number List.xlsm", 0, True)

    Dim staffTimesheet As Workbook
    Set staffTimesheet = Workbooks.Open(TheSourceFile, 0, True)
    Dim currentWorkWeek1 As String
    currentWorkWeek1 = "WW" + CStr(currentWorkWeek)
    Dim stafftimesheetCurrentWorkweek As Worksheet
    Set stafftimesheetCurrentWorkweek = staffTimesheet.Worksheets(currentWorkWeek1)
   
    
    Dim staffList As Workbook
        Set staffList = ThisWorkbook
    Dim stafflistCurrentWeek As Worksheet
        Set stafflistCurrentWeek = staffList.Worksheets("WW" & CStr(currentWorkWeek))                                  ' POINTS to the StaffList worksheet in the stafflist workbook
    '' MsgBox "Pointing at sheet:" & stafflistCurrentWeek.Name
    
    Dim mondayColumn As Integer
        mondayColumn = 7
    Dim sundayColumn As Integer
        sundayColumn = 25
    '''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''
    ' Holds the codes used for abenses, holidays etc.
    ' Every staff memeber is automatically a memeber of these workbooks and are automatically listed as being "acvtive" on these projects
    '''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''
    Dim trainingCourseInternalTrainingSelfTrainingCode As Integer
        trainingCourseInternalTrainingSelfTrainingCode = 21902
    
    Dim annualLeavePublicCompanyHoliday As Integer
        annualLeavePublicCompanyHoliday = 21904
        
    Dim sickStudyExamOtherLeaveTimeInLieu As Integer
        sickStudyExamOtherLeaveTimeInLieu = 21905
        
    '''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''

    Dim allowanceLetter1 As String
        allowanceLetter1 = "D"
    Dim allowanceLetter2 As String
        allowanceLetter2 = "d"
    Dim allowanceLetter3 As String
        allowanceLetter3 = "H"
    Dim allowanceLetter4 As String
        allowanceLetter4 = "h"
'    Dim last_row As Integer
'    last_row = projects.Worksheets("Project Numbers").Cells(Rows.count, 2).End(xlUp).Row    ' gets last used row in in in project number list
    
    Dim last_row As Long
    last_row = stafflistCurrentWeek.Cells(Rows.count, 1).End(xlUp).Row                        ' GETs the last used row in stafflist workbook
    
    If (Not IsEmpty(stafftimesheetCurrentWorkweek.Cells(12, 1).Value) And IsEmpty(stafftimesheetCurrentWorkweek.Cells(12, 2).Value) _
            And Not IsEmpty(stafftimesheetCurrentWorkweek.Cells(12, 5).Value)) Then
            '' MsgBox "Columns 1,2,5 empty"
    End If
 
    Dim count As Integer
    count = 0

    Dim counter As Integer
    counter = 0
    '' MsgBox "Timesheet name" & stafftimesheetCurrentWorkweek.Name
    
 Dim missingPLAct(500) As Variant
    
    For p = 7 To 19
    
    '' MsgBox "Row:" & p
        '''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''
        ' Conditional check to see if any row with entered hours is missing a Project number, Prpject Code P, L Description and Act. If yes count = count +1
        '''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''
        If ((Not IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 7)) Or Not IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 10)) Or Not IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 13)) _
            Or Not IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 16)) Or Not IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 19)) Or Not IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 22)) _
            Or Not IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 25)))) Then
                
                

            For Weekday1 = mondayColumn To sundayColumn
                
                '''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''
                '''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''
                ' If entry in a weekday column that is not a number then flag this and
                ' fail the PLAct test
                '''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''
                If Not IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, Weekday1)) And Not Application.WorksheetFunction.IsNumber(stafftimesheetCurrentWorkweek.Cells(p, Weekday1)) Then
                    count = count + 1
                    missingPLAct(counter) = p       ' add row # to array
                    missingPLAct(counter + 1) = "Column: " & stafftimesheetCurrentWorkweek.Cells(4, Weekday1) & " - Entry not a number"     ' add description of timesheet error to array
                    counter = counter + 2
                End If
                
                '''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''
                '''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''
                ' If entry in the "A" column is not "D","d","H" or "h" then flag as invalid entry
                '''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''

                If (Not IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, Weekday1 + 1)) And stafftimesheetCurrentWorkweek.Cells(p, Weekday1 + 1) <> allowanceLetter1) _
                    Or (Not IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, Weekday1 + 1)) And stafftimesheetCurrentWorkweek.Cells(p, Weekday1 + 1) <> allowanceLetter2) _
                       Or (Not IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, Weekday1 + 1)) And stafftimesheetCurrentWorkweek.Cells(p, Weekday1 + 1) <> allowanceLetter3) _
                            Or (Not IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, Weekday1 + 1)) And stafftimesheetCurrentWorkweek.Cells(p, Weekday1 + 1) <> allowanceLetter4) Then
                            
                    count = count + 1
                    missingPLAct(counter) = p       ' add row # to array
                    missingPLAct(counter + 1) = "Column: Allowance " & stafftimesheetCurrentWorkweek.Cells(p, Weekday1 + 1) & " - invalid choice" ' add description of timesheet error to array
                    counter = counter + 2
                End If
                Weekday1 = Weekday1 + 2
            Next Weekday1
                ''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''
                ''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''
                ' Checks if staff has entered the code for leave, absense or holiday (defined above). If yes then the Project code, P & L should be empty. Ay non empty cells are flagged and
                ' the PLAct check fails for the timsheet
                '''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''
            If (stafftimesheetCurrentWorkweek.Cells(p, 1)) = trainingCourseInternalTrainingSelfTrainingCode Or (stafftimesheetCurrentWorkweek.Cells(p, 1)) = annualLeavePublicCompanyHoliday _
                Or (stafftimesheetCurrentWorkweek.Cells(p, 1)) = sickStudyExamOtherLeaveTimeInLieu Then

                            ''''''''''''''''''''''''''''''''''''''''''''''
                            ''''''''''''''''''''''''''''''''''''''''''''''
                            ' 3 fields 1 present
                            '''''''''''''''''''''''''''''''''''''''''''''
                        If IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 2).Value) And IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 3).Value) And Not IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 4).Value) Then
                                    
                                    count = count + 1
                                    missingPLAct(counter) = p       ' add row # to array
                                    missingPLAct(counter + 1) = "L should be empty"     ' add description of timesheet error to array
                                    counter = counter + 2
                                    
                        ElseIf IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 2).Value) And IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 4).Value) And Not IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 3).Value) Then
        
                                    count = count + 1
                                    missingPLAct(counter) = p       ' add row # to array
                                    missingPLAct(counter + 1) = "P should be empty"     ' add description of timesheet error to array
                                    counter = counter + 2
                        
                        ElseIf IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 3).Value) And IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 4).Value) And Not IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 2).Value) Then
                                    
                                    count = count + 1
                                    missingPLAct(counter) = p       ' add row # to array
                                    missingPLAct(counter + 1) = "Project Code should be empty"     ' add description of timesheet error to array
                                    counter = counter + 2
                            ''''''''''''''''''''''''''''''''''''''''''''''
                            ''''''''''''''''''''''''''''''''''''''''''''''
                            ' 3 fields 2 present
                            '''''''''''''''''''''''''''''''''''''''''''''
                        ElseIf IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 2).Value) And Not IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 3).Value) And Not IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 4).Value) Then
                                    
                                    count = count + 1
                                    missingPLAct(counter) = p       ' add row # to array
                                    missingPLAct(counter + 1) = "P & L should be empty"     ' add description of timesheet error to array
                                    counter = counter + 2
                                    
                        ElseIf IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 3).Value) And Not IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 4).Value) And Not IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 2).Value) Then
        
                                    count = count + 1
                                    missingPLAct(counter) = p       ' add row # to array
                                    missingPLAct(counter + 1) = "Project Code & L should be empty"     ' add description of timesheet error to array
                                    counter = counter + 2
                        
                        ElseIf IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 4).Value) And Not IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 2).Value) And Not IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 3).Value) Then
                                    
                                    count = count + 1
                                    missingPLAct(counter) = p       ' add row # to array
                                    missingPLAct(counter + 1) = "Project Code & P should be empty"     ' add description of timesheet error to array
                                    counter = counter + 2
                                    
                            ''''''''''''''''''''''''''''''''''''''''''''''
                            ''''''''''''''''''''''''''''''''''''''''''''''
                            ' 3 fields 3 present
                            '''''''''''''''''''''''''''''''''''''''''''''
                        ElseIf Not IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 2).Value) And Not IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 3).Value) And Not IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 4).Value) Then
                
                                    count = count + 1
                                    missingPLAct(counter) = p       ' add row # to array
                                    missingPLAct(counter + 1) = "Project Code,P & L should be empty"     ' add description of timesheet error to array
                                    counter = counter + 2
                        End If
                        GoTo nextRow
            End If
            
            ''''''''''''''''''''''''''''''''
            ' 6 fields missing 5
            '''''''''''''''''''''''''''''''
            If (Not IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 1).Value) And IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 2).Value) _
                And IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 3).Value) And IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 4).Value) _
                And IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 5).Value) And IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 6).Value)) Then
                    '' MsgBox "Missing Project Number " & vbNewLine & "Row:" & p

                    count = count + 1
                    missingPLAct(counter) = p       ' add row # to array
                    missingPLAct(counter + 1) = "Missing Project code, P, L,Description and Act."     ' add description of timesheet error to array
                    counter = counter + 2
                    '' MsgBox "Missing Project code, P, L,Description and Act." & vbNewLine & "Row:" & p & vbNewLine & "Count:" & count

            ElseIf (IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 1).Value) And Not IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 2).Value) _
                And IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 3).Value) And IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 4).Value) _
                And IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 5).Value) And IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 6).Value)) Then
                    '' MsgBox "Missing Project code " & vbNewLine & "Row:" & p

                    count = count + 1
                    missingPLAct(counter) = p       ' add row # to array
                    missingPLAct(counter + 1) = "Missing Project number, P, L, Description and Act. "     ' add description of timesheet error to array
                    counter = counter + 2
                   ' ' MsgBox "Missing Project number, P, L, Description and Act. " & vbNewLine & "Row:" & p & vbNewLine & "Count:" & count

            ElseIf (IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 1).Value) And IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 2).Value) _
                And Not IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 3).Value) And IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 4).Value) _
                And IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 5).Value) And IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 6).Value)) Then
                    '' MsgBox "Missing 'P' " & vbNewLine & "Row:" & p

                    count = count + 1
                    missingPLAct(counter) = p       ' add row # to array
                    missingPLAct(counter + 1) = "Missing Project number, Project Code, L, Description and Act."     ' add description of timesheet error to array
                    counter = counter + 2
                   ' ' MsgBox "Missing Project number, Project Code, L, Description and Act." & vbNewLine & "Row:" & p & vbNewLine & "Count:" & count

            ElseIf (IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 1).Value) And IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 2).Value) _
                And IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 3).Value) And Not IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 4).Value) _
                And IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 5).Value) And IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 6).Value)) Then
                    '' MsgBox "Missing 'L' " & vbNewLine & "Row:" & p

                    count = count + 1
                    missingPLAct(counter) = p       ' add row # to array
                    missingPLAct(counter + 1) = "Missing Project number, Project Code, P, Description and Act."     ' add description of timesheet error to array
                    counter = counter + 2
                    ' MsgBox "Missing Project number, Project Code, P, Description and Act." & vbNewLine & "Row:" & p & vbNewLine & "Count:" & count

            ElseIf (IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 1).Value) And IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 2).Value) _
                And IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 3).Value) And IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 4).Value) _
                And Not IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 5).Value) And IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 6).Value)) Then
                    '' MsgBox "Missing Description " & vbNewLine & "Row:" & p

                    count = count + 1
                    missingPLAct(counter) = p       ' add row # to array
                    missingPLAct(counter + 1) = "Missing Project number, Project Code, P, L and Act."     ' add description of timesheet error to array
                    counter = counter + 2
                    ' MsgBox "Missing Project number, Project Code, P, L and Act." & vbNewLine & "Row:" & p & vbNewLine & "Count:" & count

             ElseIf (IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 1).Value) And IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 2).Value) _
                And IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 3).Value) And IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 4).Value) _
                And IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 5).Value) And Not IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 6).Value)) Then
                    '' MsgBox "Missing Act " & vbNewLine & "Row:" & p

                    count = count + 1
                    missingPLAct(counter) = p       ' add row # to array
                    missingPLAct(counter + 1) = "Missing Project number, Project Code, P, L and Description "     ' add description of timesheet error to array
                    counter = counter + 2
                    ' MsgBox "Missing Project number, Project Code, P, L and Description " & vbNewLine & "Row:" & p & vbNewLine & "Count:" & count
             
    '''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''
     ' 6 fields missing 4
    '''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''

            ElseIf (Not IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 1).Value) And Not IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 2).Value) _
                And IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 3).Value) And IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 4).Value) _
                And IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 5).Value) And IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 6).Value)) Then
                    '' MsgBox "Missing P, L., Description & Act " & vbNewLine & "Row:" & p

                    count = count + 1
                    missingPLAct(counter) = p       ' add row # to array
                    missingPLAct(counter + 1) = "Missing P, L., Description & Act "     ' add description of timesheet error to array
                    counter = counter + 2
                    ' MsgBox "Missing P, L., Description & Act " & vbNewLine & "Row:" & p & vbNewLine & "Count:" & count

            ElseIf (Not IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 1).Value) And IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 2).Value) _
                And IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 3).Value) And Not IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 4).Value) _
                And IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 5).Value) And IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 6).Value)) Then
                    '' MsgBox "Missing Project code, L., Description & Act " & vbNewLine & "Row:" & p

                    count = count + 1
                    missingPLAct(counter) = p       ' add row # to array
                    missingPLAct(counter + 1) = "Missing Project code, L., Description & Act "     ' add description of timesheet error to array
                    counter = counter + 2
                    ' MsgBox "Missing Project code, L., Description & Act " & vbNewLine & "Row:" & p & vbNewLine & "Count:" & count

            ElseIf (Not IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 1).Value) And IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 2).Value) _
                And IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 3).Value) And Not IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 4).Value) _
                And IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 5).Value) And IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 6).Value)) Then
                    '' MsgBox "Missing Project Code, P., Description & Act " & vbNewLine & "Row:" & p

                    count = count + 1
                    missingPLAct(counter) = p       ' add row # to array
                    missingPLAct(counter + 1) = "Missing Project Code, P., Description & Act "     ' add description of timesheet error to array
                    counter = counter + 2
                    ' MsgBox "Missing Project Code, P., Description & Act " & vbNewLine & "Row:" & p & vbNewLine & "Count:" & count

            ElseIf (Not IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 1).Value) And IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 2).Value) _
                And IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 3).Value) And IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 4).Value) _
                And Not IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 5).Value) And IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 6).Value)) Then
                    '' MsgBox "Missing Project Code, P, L & Act " & vbNewLine & "Row:" & p

                    count = count + 1
                    missingPLAct(counter) = p       ' add row # to array
                    missingPLAct(counter + 1) = "Missing Project Code, P, L & Act "     ' add description of timesheet error to array
                    counter = counter + 2
                    ' MsgBox "Missing Project Code, P, L & Act " & vbNewLine & "Row:" & p & vbNewLine & "Count:" & count

            ElseIf (Not IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 1).Value) And IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 2).Value) _
                And IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 3).Value) And IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 4).Value) _
                And IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 5).Value) And Not IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 6).Value)) Then
                    '' MsgBox "Missing Project Code, P, L & Description " & vbNewLine & "Row:" & p

                    count = count + 1
                    missingPLAct(counter) = p       ' add row # to array
                    missingPLAct(counter + 1) = "Missing Project Code, P, L & Description "     ' add description of timesheet error to array
                    counter = counter + 2
                    ' MsgBox "Missing Project Code, P, L & Description  " & vbNewLine & "Row:" & p & vbNewLine & "Count:" & count

            ElseIf (IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 1).Value) And Not IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 2).Value) _
                And Not IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 3).Value) And IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 4).Value) _
                And IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 5).Value) And IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 6).Value)) Then
                    '' MsgBox "Missing Project Number, L , Description & Act" & vbNewLine & "Row:" & p

                    count = count + 1
                    missingPLAct(counter) = p       ' add row # to array
                    missingPLAct(counter + 1) = "Missing Project Number, L , Description & Act"     ' add description of timesheet error to array
                    counter = counter + 2
                    ' MsgBox "Missing Project Number, L , Description & Act" & vbNewLine & "Row:" & p & vbNewLine & "Count:" & count

            ElseIf (IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 1).Value) And Not IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 2).Value) _
                And IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 3).Value) And Not IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 4).Value) _
                And IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 5).Value) And IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 6).Value)) Then
                    '' MsgBox "Missing Project Number, P , Description & Act" & vbNewLine & "Row:" & p

                    count = count + 1
                    missingPLAct(counter) = p       ' add row # to array
                    missingPLAct(counter + 1) = "Missing Project Number, P , Description & Act"     ' add description of timesheet error to array
                    counter = counter + 2
                    ' MsgBox "Missing Project Number, P , Description & Act" & vbNewLine & "Row:" & p & vbNewLine & "Count:" & count

            ElseIf (IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 1).Value) And Not IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 2).Value) _
                And IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 3).Value) And IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 4).Value) _
                And Not IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 5).Value) And IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 6).Value)) Then
                    '' MsgBox "Missing Project Number, P , L & Act" & vbNewLine & "Row:" & p

                    count = count + 1
                    missingPLAct(counter) = p       ' add row # to array
                    missingPLAct(counter + 1) = "Missing Project Number, P , L & Act"     ' add description of timesheet error to array
                    counter = counter + 2
                    ' MsgBox "Missing Project Number, P , L & Act" & vbNewLine & "Row:" & p & vbNewLine & "Count:" & count

            ElseIf (IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 1).Value) And Not IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 2).Value) _
                And IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 3).Value) And IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 4).Value) _
                And IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 5).Value) And Not IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 6).Value)) Then
                    '' MsgBox "Missing Project Number, P , L & Description" & vbNewLine & "Row:" & p

                    count = count + 1
                    missingPLAct(counter) = p       ' add row # to array
                    missingPLAct(counter + 1) = "Missing Project Number, P , L & Description"     ' add description of timesheet error to array
                    counter = counter + 2
                    ' MsgBox "Missing Project Number, P , L & Description" & vbNewLine & "Row:" & p & vbNewLine & "Count:" & count

            ElseIf (IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 1).Value) And IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 2).Value) _
                And Not IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 3).Value) And Not IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 4).Value) _
                And IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 5).Value) And IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 6).Value)) Then
                    '' MsgBox "Missing Project Number, Project Code,  Description & Act." & vbNewLine & "Row:" & p

                    count = count + 1
                    missingPLAct(counter) = p       ' add row # to array
                    missingPLAct(counter + 1) = "Missing Project Number, Project Code,  Description & Act."     ' add description of timesheet error to array
                    counter = counter + 2
                    ' MsgBox "Missing Project Number, Project Code,  Description & Act." & vbNewLine & "Row:" & p & vbNewLine & "Count:" & count

            ElseIf (IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 1).Value) And IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 2).Value) _
