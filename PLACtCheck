                And Not IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 3).Value) And Not IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 4).Value) _
                And Not IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 5).Value) And Not IsEmpty(stafftimesheetCurrentWorkweek.Cells(p, 6).Value)) Then
